              <stringProp name="parameters"></stringProp>
              <stringProp name="script">import groovy.json.JsonSlurper
import groovy.json.JsonOutput

def slurper = new JsonSlurper()
def originalJson = vars.get("movieJson")
if (originalJson == null) {
    log.error("movieJson variable is null!")
    return
}
def movie = slurper.parseText(originalJson)

// Увеличиваем число оскаров на 1
int currentOscars = movie.oscarsCount == null ? 0 : movie.oscarsCount
movie.oscarsCount = currentOscars + 1

sampler.getArguments().getArgument(0).setValue(JsonOutput.toJson(movie))</stringProp>
